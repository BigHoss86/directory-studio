  <name>Apache Directory Studio RCP Help</name>
  <packaging>jar</packaging>

  <description />

  <properties>
    <current_pom_version>${pom.version}</current_pom_version>
  </properties>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-eclipse-plugin</artifactId>
